{
  "expo": {
    "name": "NBA Today",
    "slug": "nba-today-react-native",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }, 
    "extra": {
      "nba": {
        "production": {
          "apiServerUrl": "md-nba-today.herokuapp.com",
          "httpProtocol": "https"
       
        },
        "development": {
          "apiServerUrl": "md-nba-today.herokuapp.com",
          "httpProtocol": "http"         
        }
      }
    }
  }
}
